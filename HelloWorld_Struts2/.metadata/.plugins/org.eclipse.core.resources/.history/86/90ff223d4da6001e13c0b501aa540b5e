package com.bjsxt.struts2.ognl;

public class Dog {
	
	public Dog() {
		
	}
	
	public Dog(String name) {
		super();
		this.name = name;
	}

	private String name;
	
	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	@Override
	public String toString() {
		return "dog: " + name;
	}
}
